{
  "name": "primum-ai-sales-orchestration",
  "version": "1.0.0",
  "description": "Human-Like AI Sales Orchestration Platform for Personal Loans (BFSI)",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "dev:frontend": "cd frontend && npm start",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build"
  },
  "keywords": ["ai", "sales", "orchestration", "bfsi", "personal-loans"],
  "author": "EY Techathon Team",
  "license": "MIT"
}